<odoo>
	<record id="survey_form" model="ir.ui.view">
		<field name="name">survey.survey.view.form.inherit</field>
		<field name="model">survey.survey</field>
		<field name="inherit_id" ref="viin_survey_crm.survey_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='create_lead']/parent::group" position="after">
				<group>
					<field name="admission_id"/>
				</group>
			</xpath>
		</field>
	</record>
</odoo>
